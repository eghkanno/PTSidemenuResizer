{
    "manifest_version": 2,
    "name": "Resizable side menu for Pivotal Tracker",
    "version": "1.0",
    "description": "Makes Pivotal Tracker's side menu resizable.",
    "author": "budoucha",
    "content_scripts": [
        {
            "matches": [
                "http://www.pivotaltracker.com/*",
                "https://www.pivotaltracker.com/*"
            ],
            "js": [
                "jquery-3.3.1.min.js",
                "jquery.funcResizeBox.js",
                "script.js"
            ]
        }
    ],
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}